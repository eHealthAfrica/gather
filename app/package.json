  "engines": {
    "node": ">=6.0.0",
    "npm": ">=3.0.0"
  },

  "scripts": {
    "lint-js": "standard './gather/assets/apps/**/*.jsx'",
    "lint-scss": "sass-lint --config ./conf/extras/sass-lint.yml --verbose",
    "lint": "npm run lint-js && npm run lint-scss",
    "mocha": "mocha --require ./gather/assets/tests/mocha.setup.js './gather/assets/apps/**/*.spec.jsx'",
    "test": "npm run lint && npm run mocha",
    "webpack": "webpack --config ./gather/webpack.prod.js",
    "webpack-server": "node ./gather/webpack.server.js"
  },

  "dependencies": {},
  "devDependencies": {
    "babel-core": "~6.26.0",
    "babel-loader": "~7.1.0",
    "babel-preset-es2015": "~6.24.0",
    "babel-preset-react": "~6.24.0",
    "babel-preset-stage-2": "~6.24.0",
    "bootstrap": "~4.0.0-beta.2",
    "css-loader": "~0.28.0",
    "extract-text-webpack-plugin": "~3.0.0",
    "file-loader": "~1.1.0",
    "font-awesome": "~4.7.0",
    "html5shiv": "~3.7.0",
    "jquery": "~3.3.0",
    "jsdom": "~11.6.0",
    "mocha": "~5.0.0",
    "moment": "~2.20.0",
    "nock": "~9.1.0",
    "node-fetch": "~2.0.0",
    "node-sass": "~4.7.0",
    "popper.js": "~1.12.0",
    "react": "~16.2.0",
    "react-dom": "~16.2.0",
    "react-hot-loader": "~3.1.0",
    "react-intl": "~2.4.0",
    "react-filtered-multiselect": "~0.5.0",
    "sass-lint": "~1.12.0",
    "sass-loader": "~6.0.0",
    "standard": "~11.0.0",
    "style-loader": "~0.20.0",
    "url-loader": "~0.6.0",
    "webpack": "~3.11.0",
    "webpack-bundle-tracker": "~0.2.0",
    "webpack-dev-server": "~2.11.0",
    "whatwg-fetch": "~2.0.0"
  }
}
